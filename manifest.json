{
  "manifest_version": 2,
  "name": "CS:GO Inventory Assistant",
  "description": "A browser extension to provide a better CS:GO inventory experience.",
  "permissions": [
    "*://api.csgofloat.com/*",
    "*://cs.deals/API/*",
    "*://api.csgoskins.gg/*"
  ],
  "content_scripts": [
    {
      "matches": [ "*://steamcommunity.com/*/inventory*" ],
      "js": [ "main.js" ]
    }
  ]
}