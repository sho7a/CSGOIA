{
  "name": "csgoia",
  "version": "0.4.1",
  "description": "A browser extension to provide a better CS:GO inventory experience.",
  "keywords": [
    "csgo",
    "browser-extension",
    "csgo-inventory"
  ],
  "homepage": "https://github.com/PryosCode/CSGOIA",
  "license": "Apache-2.0",
  "private": true,
  "author": {
    "name": "PryosCode",
    "email": "info@pryoscode.net",
    "url": "https://pryoscode.net"
  },
  "bugs": {
    "url": "https://github.com/PryosCode/CSGOIA/issues",
    "email": "info@pryoscode.net"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PryosCode/CSGOIA.git"
  },
  "scripts": {
    "dev:bundle": "webpack watch --mode=development",
    "dev:browser": "web-ext run --source-dir build -u https://steamcommunity.com/id/PryosCode/inventory#730",
    "dev": "concurrently \"yarn dev:bundle\" \"yarn dev:browser\"",
    "build:bundle": "webpack --mode=production",
    "build:pack": "web-ext build --overwrite-dest --source-dir build",
    "build": "yarn build:bundle && yarn build:pack"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@babel/register": "^7.16.5",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.5.1",
    "copy-webpack-plugin": "^10.2.0",
    "eslint": "^8.5.0",
    "eslint-webpack-plugin": "^3.1.1",
    "typescript": "^4.5.4",
    "web-ext": "^6.6.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
}