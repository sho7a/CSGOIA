{
  "manifest_version": 2,
  "name": "CS:GO Inventory Assistant",
  "version": "0.1.0",
  "description": "A browser extension to provide a better CS:GO inventory experience.",
  "permissions": [
    "https://api.csgofloat.com/*",
    "https://api.csgoskins.gg/*"
  ],
  "content_scripts": [
    {
      "matches": [ "https://steamcommunity.com/*/inventory*" ],
      "js": [ "main.js" ]
    }
  ]
}